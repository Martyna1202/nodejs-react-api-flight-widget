{
  "functions": {
    "api/*.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "source": "/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
